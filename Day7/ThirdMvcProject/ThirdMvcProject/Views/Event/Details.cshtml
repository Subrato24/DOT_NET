
@{
    ViewBag.Title = "Details";
}

<table class="table table-hover table-striped">
    <tbody>
        <tr>
            <th>Event Id</th>
            <td>@Model.eventId</td>
        </tr>
        <tr>
            <th>Event Name</th>
            <td>@Model.eventName</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>@Model.description</td>
        </tr>
        <tr>
            <th>Start Date</th>
            <td>@Model.startdate</td>
        </tr>
        <tr>
            <th>End Date</th>
            <td>@Model.enddate</td>
        </tr>
        <tr>
            <th>Fees</th>
            <td>@Model.fees</td>
        </tr>
        <tr>
            <th>total Seats</th>
            <td>@Model.totalseatfilled</td>
        </tr>
       
        <tr>
            <th>Code</th>
            <td>@Model.code</td>
        </tr>
    </tbody>
</table>